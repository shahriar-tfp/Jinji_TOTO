									<Action Type="Report" ID="actChartSizeNarrower">
										<Target Type="Report" KeepScrollPosition="True" KeepShowElements="True"/>
										<LinkParams rdOgCommand="ChartNarrower" rdOgCommandID="@Function.GUID~"/>
									</Action>
								</Label>
								<Label Caption=" | " ID="lblSep"/>
								<Label Caption="Wider" ID="lblChartSizeWider">
									<Action Type="Report" ID="actChartSizeWider">
										<Target Type="Report" KeepScrollPosition="True" KeepShowElements="True"/>
