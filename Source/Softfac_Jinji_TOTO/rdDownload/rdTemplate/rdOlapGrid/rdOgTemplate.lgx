										<EventHandler DhtmlEvent="onchange">
											<Action Type="Report" ID="actChartStyle">
												<Target Type="Report" KeepScrollPosition="True" KeepShowElements="True"/>
												<LinkParams rdOgCommand="ChartTitle" rdOgCommandID="@Function.GUID~"/>
											</Action>
										</EventHandler>
									</InputText>
									<Spaces Size="3"/>
									<Label Caption="Set Title" ID="lblChartSetTitle">
										<Action Type="Report" ID="actChartTitle">
											<Target Type="Report" KeepScrollPosition="True" KeepShowElements="True"/>
											<LinkParams rdOgCommand="ChartTitle" rdOgCommandID="@Function.GUID~"/>
										</Action>
									</Label>
								</Division>
							</Column>
						</Row>
						<Row ID="rowContentChart">
							<Column ColSpan="2">
