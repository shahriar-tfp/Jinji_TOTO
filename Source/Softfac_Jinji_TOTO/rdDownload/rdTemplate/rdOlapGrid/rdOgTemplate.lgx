									<Chart Type="XY" XYChartType="Bar" ChartHeight="400" ChartWidth="600" BorderLeft="75" BorderTop="25" BorderBottom="150" BorderRight="150" Transparency="3" LineWidth="2" ID="ogChart" BackgroundColor="Transparent" ChartBackgroundColor="White" BarShape="Circle" Color="red, yellow, blue">
